{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "STLink Debug",
            "type": "cortex-debug",
            "request": "launch",
            // Resolved by CMake Tools:
            "executable": "${command:cmake.launchTargetPath}",
            "svdFile": "${command:cmake.getLaunchTargetDirectory}/device.svd",
            "servertype": "stlink",
            "runToEntryPoint": "main"
        },
        {
            "name": "Picoprobe Debug",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "gdbTarget": "localhost:3333",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/stm32l0.cfg"
            ],
            "showDevDebugOutput": "raw",

            // Resolved by CMake Tools:
            "executable": "${command:cmake.launchTargetPath}",
            "svdFile": "${command:cmake.getLaunchTargetDirectory}/device.svd",
        },
    ]
}